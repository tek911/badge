{
  "name": "@shared/sdk",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --dts",
    "dev": "tsup src/index.ts --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "1.12.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "tsup": "8.5.0",
    "typescript": "5.3.3",
    "vitest": "3.2.4",
    "@types/node": "22.18.6"
  },
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*"
  }
}
