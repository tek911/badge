{
  "name": "@shared/sdk",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --dts",
    "dev": "tsup src/index.ts --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "1.6.7",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "tsup": "7.2.0",
    "typescript": "5.3.3",
    "vitest": "1.2.2"
  },
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*"
  }
}